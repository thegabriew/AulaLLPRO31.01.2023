#include<stdio.h>
#include<stdlib.h>

int main (){
	
	char c,f[50]; 
	int t=0;
	printf("Digite um frase: ");
	gets(f);
	printf("Digite um caractere: ");
	scanf("%s",&c);
		
	for (int idx=0; idx<50; idx++){
	if(f[idx]== c){
		
	t=t +1;
	
	}
	if(f[idx]=='\0'){
		break;
	}
	}
	printf("\n%d",t);
	
	return 0;
}

